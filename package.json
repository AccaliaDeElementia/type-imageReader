    "eslint-plugin-promise": "^5.1.0",
    "find": "^0.3.0",
    "fs-extra": "^10.0.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
