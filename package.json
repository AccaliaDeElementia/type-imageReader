{
  "name": "type-imagereader",
  "version": "1.0.0",
  "description": "A typscript powered web based image and comic reader pulling from local files",
  "main": "index.js",
  "scripts": {
    "preclean": "npm run lint",
    "clean": "ts-node build.ts",
    "prebuild": "npm run clean",
    "build": "tsc --build tsconfig.json",
    "lint": "eslint . --ext .ts,.js",
    "start": "cd ./dist && node .; cd ..",
    "pretest": "npm run build",
    "test": "cd ./src && ts-node .; cd ..",
    "knex": "knex --knexfile src/knexfile.ts",
    "predev": "npm run lint",
    "dev": "cd ./src && DEBUG=type-imagereader:* ts-node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AccaliaDeElementia/type-imagereader.git"
  },
  "author": "Accalia Elementia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AccaliaDeElementia/type-imagereader/issues"
  },
  "homepage": "https://github.com/AccaliaDeElementia/type-imagereader#readme",
  "dependencies": {
    "@types/debug": "^4.1.7",
    "command-line-args": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "jet-logger": "^1.0.6",
    "knex": "^0.21.21",
    "morgan": "^1.10.0",
    "node-sass": "^7.0.1",
    "node-sass-middleware": "^1.0.1",
    "pg": "^8.7.1",
    "postgresql": "0.0.1",
    "pug": "^3.0.2",
    "sharp": "^0.28.3",
    "socket.io": "^4.4.1",
    "words-to-numbers": "^1.5.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.12",
    "@types/jasmine": "^3.8.2",
    "@types/jsonfile": "^6.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.6.0",
    "@types/node-sass-middleware": "^0.0.32",
    "@types/sharp": "^0.28.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "find": "^0.3.0",
    "fs-extra": "^10.0.0",
    "ts-node": "^10.2.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
