{
  "name": "type-imagereader",
  "version": "1.0.0",
  "description": "A typscript powered web based image and comic reader pulling from local files",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "tsc --build tsconfig.json",
    "lint": "eslint . --ext .ts,.js",
    "start": "ts-node .",
    "pretest": "npm run build",
    "test": "nyc mocha",
    "knex": "knex --knexfile knexfile.ts",
    "predev": "npm run build",
    "dev": "DEBUG=type-imagereader:* SKIP_SYNC=1 ts-node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AccaliaDeElementia/type-imagereader.git"
  },
  "author": "Accalia Elementia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AccaliaDeElementia/type-imagereader/issues"
  },
  "homepage": "https://github.com/AccaliaDeElementia/type-imagereader#readme",
  "dependencies": {
    "@types/pug": "^2.0.6",
    "brfs": "^2.0.2",
    "browserify": "^17.0.0",
    "command-line-args": "^5.2.1",
    "common-shakeify": "^1.1.1",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "helmet": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "jet-logger": "^1.1.5",
    "knex": "^2.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "postgresql": "0.0.1",
    "pug": "^3.0.2",
    "sass": "^1.54.0",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.30.6",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "terser": "^5.14.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "tsify": "^5.0.4",
    "typescript": "^4.7.4",
    "words-to-numbers": "^1.5.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testdeck/mocha": "^0.2.0",
    "@types/browserify": "^12.0.37",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jasmine": "^4.0.3",
    "@types/jsdom": "^20.0.0",
    "@types/jsonfile": "^6.1.0",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.45",
    "@types/serve-favicon": "^2.5.3",
    "@types/sharp": "^0.30.4",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.18.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "jsdom": "^20.0.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.21"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "**/*.ts",
      "utils/*.ts"
    ],
    "exclude": [
      "**/*.spec.ts",
      "knexfile.ts"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "all": true
  }
}
