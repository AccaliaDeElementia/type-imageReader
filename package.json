  },
  "homepage": "https://github.com/AccaliaDeElementia/type-imagereader#readme",
  "dependencies": {
    "@types/debug": "^4.1.5",
    "@types/sharp": "^0.27.3",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "fs-extra": "^9.1.0",
    "helmet": "^4.4.1",
    "http-status-codes": "^2.1.4",
    "jet-logger": "^1.0.4",
    "knex": "^0.21.19",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-sass-middleware": "^0.11.0",
    "pg": "^8.5.1",
    "postgresql": "0.0.1",
