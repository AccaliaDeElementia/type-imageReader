    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.12",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.11",
    "@types/jasmine": "^3.7.4",
    "@types/jsonfile": "^6.0.0",
    "@types/morgan": "^1.9.2",
