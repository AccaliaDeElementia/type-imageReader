{
  "name": "type-imagereader",
  "version": "1.0.0",
  "description": "A typscript powered web based image and comic reader pulling from local files",
  "main": "index.js",
  "scripts": {
    "preclean": "npm run lint",
    "clean": "ts-node build.ts",
    "prebuild": "npm run clean",
    "build": "tsc --build tsconfig.json",
    "lint": "eslint . --ext .ts,.js",
    "start": "cd ./dist && node .; cd ..",
    "pretest": "npm run build",
    "test": "cd ./src && ts-node .; cd ..",
    "knex": "knex --knexfile src/knexfile.ts",
    "predev": "npm run lint",
    "dev": "cd ./src && DEBUG=type-imagereader:* ts-node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AccaliaDeElementia/type-imagereader.git"
  },
  "author": "Accalia Elementia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AccaliaDeElementia/type-imagereader/issues"
  },
  "homepage": "https://github.com/AccaliaDeElementia/type-imagereader#readme",
  "dependencies": {
    "@types/debug": "^4.1.7",
    "command-line-args": "^5.2.1",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "jet-logger": "^1.1.5",
    "knex": "^2.1.0",
    "morgan": "^1.10.0",
    "node-sass": "^7.0.1",
    "node-sass-middleware": "^1.0.1",
    "pg": "^8.7.3",
    "postgresql": "0.0.1",
    "pug": "^3.0.2",
    "sharp": "^0.30.6",
    "socket.io": "^4.5.1",
    "words-to-numbers": "^1.5.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jasmine": "^4.0.3",
    "@types/jsonfile": "^6.1.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.40",
    "@types/node-sass-middleware": "^0.0.32",
    "@types/sharp": "^0.30.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.17.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "find": "^0.3.0",
    "fs-extra": "^10.1.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  }
}
